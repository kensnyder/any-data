  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@vitest/coverage-istanbul": "2.0.2",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "rimraf": "6.0.1",
    "typescript": "5.5.3",
    "vitest": "2.0.2"
  }
