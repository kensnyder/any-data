{
  "name": "any-data",
  "version": "1.0.1",
  "description": "Convert data of any type to text, json, formData, TypedArray, Blob, or ArrayBuffer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/any-data.git"
  },
  "keywords": [],
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest",
    "build": "npm run build:clean && npm run build:dts && npm run build:esm && npm run build:cjs",
    "build:clean": "rimraf dist/ && mkdir dist && touch dist/.gitkeep",
    "build:dts": "yes | npx dts-bundle-generator -o dist/index.d.ts index.ts",
    "build:esm": "yes | npx esbuild index.ts --bundle --platform=node --format=esm --outfile=dist/index.mjs",
    "build:cjs": "yes | npx esbuild index.ts --bundle --platform=node --format=cjs --outfile=dist/index.cjs",
    "coverage": "vitest run --coverage",
    "view-coverage": "open ./coverage/lcov-report/index.html"
  },
  "author": "Ken Snyder <kendsnyder@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "is-plain-object": "5.0.0"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "@vitest/coverage-v8": "2.0.4",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "rimraf": "6.0.1",
    "type-fest": "4.23.0",
    "typescript": "5.5.4",
    "vitest": "2.0.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
